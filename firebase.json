{
  "hosting": {
    "headers": [
      {
        "source": "/service-worker.js",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-cache"
          }
        ]
      }
    ],
    "target": "panos.pan.dev",
    "public": "build",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "redirects": [
      {
        "source": "/api/tp",
        "destination": "https://pan.dev/cdss/threat-vault/api",
        "type": 301
      },
      {
        "source": "/api/tp/*",
        "destination": "https://pan.dev/cdss/threat-vault/api",
        "type": 301
      }
    ]
  }
}
